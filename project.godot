[application]

config/name="Kings Valley"
run/main_scene="res://scenes/World/World.tscn"
boot_splash/image="res://resources/sprites/splash.png"
config/icon="res://kvicon.png"

[autoload]

Utils="*res://global/Utils.gd"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[importer_defaults]

vnen.tiled_importer={
"add_background": true,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": true,
"image_flags": 0,
"post_import_script": "res://resources/levels/PostImport.gd",
"save_tiled_properties": true,
"tile_metadata": false,
"uv_clip": true
}

